{
  "openapi": "3.0.1",
  "info": {
    "title": "Home Center",
    "description": "Framily API",
    "version": "v1"
  },
  "servers": [
    {
      "url": "/api"
    }
  ],
  "paths": {
    "/panels/family": {
      "get": {
        "tags": ["family panel"],
        "summary": "Get users family",
        "operationId": "getFamily",
        "parameters": [
          {
            "name": "userID",
            "in": "query",
            "description": "Request userID",
            "required": true,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "from",
            "in": "query",
            "description": "Request timestamp for from",
            "required": true,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "to",
            "in": "query",
            "description": "Request timestamp for to",
            "required": true,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Returns user locations data",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/FamilyLocation"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "FamilyLocation": {
        "type": "object",
        "properties": {
          "timestamp": {
            "type": "integer"
          },
          "latitude": {
            "type": "number"
          },
          "longitude": {
            "type": "number"
          }
        },
        "example": {
          "latitude": 6.027456183070403,
          "timestamp": 0,
          "longitude": 1.4658129805029452
        }
      }
    }
  }
}
