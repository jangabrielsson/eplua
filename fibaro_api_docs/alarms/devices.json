{
  "openapi": "3.0.1",
  "info": {
    "title": "Api documentation",
    "description": "Get alarm devices API",
    "version": "v1"
  },
  "servers": [
    {
      "url": "/api"
    }
  ],
  "paths": {
    "/alarms/v1/devices/": {
      "get": {
        "tags": ["alarms"],
        "summary": "Get alarm devices",
        "operationId": "getAlarmDevices",
        "responses": {
          "200": {
            "description": "Returns list of devices that may be added to partition",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/AlarmDeviceEntryDto"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AlarmDeviceEntryDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "name": {
            "type": "string"
          }
        },
        "example": {
          "name": "name",
          "id": 123
        }
      }
    }
  }
}
