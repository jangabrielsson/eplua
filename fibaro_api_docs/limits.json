{
  "openapi": "3.0.1",
  "info": {
    "title": "Home Center",
    "description": "Limits API",
    "version": "v1"
  },
  "servers": [
    {
      "url": "/api"
    }
  ],
  "paths": {
    "/limits": {
      "get": {
        "tags": ["limits"],
        "summary": "Get limits object",
        "operationId": "getLimits",
        "responses": {
          "200": {
            "description": "Returns limits data",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Limits"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
